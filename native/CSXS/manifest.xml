<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   ExtensionBundleName="Pexels"
                   ExtensionBundleId="com.github.kluman.pexels"
                   ExtensionBundleVersion="1.0.2"
                   Version="6.0">
    <ExtensionList>
        <Extension Id="com.github.kluman.pexels" Version="1.0.2"/>
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="IDSN" Version="13"/>
        </HostList>
        <LocaleList>
            <Locale Code="All"/>
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="9.0"/>
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="com.github.kluman.pexels">
            <DispatchInfo>
                <Resources>
                    <MainPath>./index.html</MainPath>
                    <ScriptPath>./extendscript/pexels.js</ScriptPath>
                    <CEFCommandLine>
                        <Parameter>--enable-nodejs</Parameter>
                    </CEFCommandLine>
                </Resources>
                <Lifecycle>
                    <AutoVisible>false</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>Pexels</Menu>
                    <Geometry>
                        <Size>
                            <Height>800</Height>
                            <Width>615</Width>
                        </Size>
                        <MaxSize>
                            <Height>800</Height>
                            <Width>750</Width>
                        </MaxSize>
                        <MinSize>
                            <Height>700</Height>
                            <Width>615</Width>
                        </MinSize>
                    </Geometry>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>
